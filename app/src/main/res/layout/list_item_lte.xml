<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/list_pci_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/list_dbm_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/list_pci_text_view"
        android:textSize="20sp"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/list_asulevel_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/list_dbm_text_view"
        android:textSize="20sp"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/list_timing_advance_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/list_asulevel_text_view"
        android:textSize="20sp"
        android:textColor="@color/white" />

</RelativeLayout>